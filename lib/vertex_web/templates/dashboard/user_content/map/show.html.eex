<h1>Show Map</h1>

<ul>

  <li>
    <strong><%=gettext("Name") %>: </strong><%= @map.name %>
  </li>

  <li>
    <strong><%=gettext("Description") %>: </strong><%= @map.description %>
  </li>

  <li>
    <strong><%=gettext("Is Public") %>: </strong><%= @map.is_public %>
  </li>

</ul>

<span><%= link gettext("Edit"), to: Routes.dashboard_map_path(@conn, :edit, @map) %></span>
<span><%= link gettext("Back"), to: Routes.dashboard_map_path(@conn, :index) %></span>

{
    debug
    auto_https disable_redirects
}

http://vsekai.local {
	handle_path /api/v1/* {
		reverse_proxy uro:4000
	}

	handle_path /* {
		reverse_proxy nextjs:3000
	}
}

https://vsekai.local {
	handle_path /api/v1/* {
		reverse_proxy uro:4000
	}

	handle_path /* {
		reverse_proxy nextjs:3000
	}
}
